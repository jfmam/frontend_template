{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "ignorePatterns": ["/**/*.stories.*"],
  "overrides": [
    {
      "files": [
        "./**/*.@(jsx|tsx)"
      ],
      "rules": {
        "import/no-anonymous-default-export": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
      }
    }
  ]
}
